{
  "name": "app",
  "version": "0.0.1",
  "type": "module",
  "description": "This is an example of a Crawlee project.",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@types/jest": "^29.5.11",
    "@types/luxon": "^3.4.2",
    "crawlee": "^3.0.0",
    "crypto": "^1.0.1",
    "jest": "^29.7.0",
    "luxon": "^3.4.4",
    "pino": "^8.17.2",
    "playwright": "*",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@apify/tsconfig": "^0.1.0",
    "@types/node": "^18.19.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "~5.3.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node dist/src/main.js",
    "start:dev": "tsx src/main.ts",
    "build": "rm -rf ./dist && tsc --project tsconfig.prod.json && tsc-alias -p tsconfig.json",
    "db:seed": "tsx prisma/seeders/run.ts",
    "db:reset": "prisma migrate reset --force",
    "db.test:reset": "DATABASE_URL=postgresql://postgres:password@db_test:5432/crawler?schema=public prisma migrate reset --force",
    "test": "npm run db.test:reset && jest --runInBand"
  },
